{
    "name": "wp-plugins/security-check",
    "description": "A WordPress plugin for performing security checks.",
    "license": "GPL-2.0-or-later",
    "autoload": {
        "psr-4": {
            "WpPlugins\\SecurityCheck\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Karishma Shukla",
            "email": "108402893+karishma-shukla-08@users.noreply.github.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "phpgangsta/googleauthenticator": "dev-master"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7.0",
        "wp-coding-standards/wpcs": "^3.1",
        "phpunit/phpunit": "^11.5"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
